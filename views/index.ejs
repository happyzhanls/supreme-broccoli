<% include ./partials/header %>
<h1>Index page</h1>

<% blogs.forEach(function(blog) { %>
       <div>
           <h2><%= blog.title %></h2>
           <img src=<%= blog.image %>>
           <p><%= blog.body %></p>
           <p>Created at <%= blog.created %></p>
       </div>
<% }) %>
<% include ./partials/footer %>
